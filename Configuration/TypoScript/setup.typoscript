
plugin.tx_rkwmaps {
  view {
    templateRootPaths.0 = EXT:rkw_maps/Resources/Private/Templates/
    templateRootPaths.1 = {$plugin.tx_rkwmaps.view.templateRootPath}
    partialRootPaths.0 = EXT:rkw_maps/Resources/Private/Partials/
    partialRootPaths.1 = {$plugin.tx_rkwmaps.view.partialRootPath}
    layoutRootPaths.0 = EXT:rkw_maps/Resources/Private/Layouts/
    layoutRootPaths.1 = {$plugin.tx_rkwmaps.view.layoutRootPath}
  }
  persistence {
    storagePid = {$plugin.tx_rkwmaps.persistence.storagePid}
    #recursive = 1
  }
  features {
    #skipDefaultArguments = 1
  }
  mvc {
    #callDefaultActionIfActionCantBeResolved = 1
  }
  settings {
    title = {$plugin.tx_rkwmaps.settings.title}
    data = {$plugin.tx_rkwmaps.settings.data}
  }
}


